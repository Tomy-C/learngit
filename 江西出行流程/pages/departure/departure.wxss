.main{
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
.addre{
  display: flex;
  width: 95%;
  margin:0 auto;
  justify-content: space-around;
}
.goto{
  width: 30%;
  background-color:#fff;
  border-radius: 3px;
  text-align: center;
  margin: 5px;
  font-size: 30rpx;
}
input{
  background-color: white;
  margin: 5px;
}
.showhi{
  width: 90%;
  height: 1000rpx;
  margin: 0 auto;
  background-color: #fff;
}
.clock{
  width: 10px;
  height: 10px;
  margin: 20rpx;
  vertical-align: middle
}
.add{
  vertical-align: middle;
  color: #999;
  font-size: 30rpx;
  margin: 5px 0;
}
.add-list{
  border-bottom: 1px solid #999;
}